{% extends 'base.twig' %}
{% block content %}
    {% for article in articles %}
        <h1>{{ article.title }}</h1>
        <p>
            <em>Published by admin at {{ article.date }} </em>
        </p>
        <p>
            {{ article.content }}
        </p>
        <a href="article/{{ article.id }}" class="btn btn-primary">Read more</a>
    {% endfor %}
{% endblock %}